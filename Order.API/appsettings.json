{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConsulSetting": {
    "ServiceName": "OrderService",
    "ServiceIP": "localhost",
    "ServiceHealthCheck": "/healthcheck",
    // 注意，docker容器內部無法使用localhost訪問宿主機器，如果是控制台啟動的話就用localhost
    "ConsulAddress": "http://host.docker.internal:8500"
  }
}
